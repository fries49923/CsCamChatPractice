<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Color-->
    <Color x:Key="Color.White.alpha16">#10FFFFFF</Color>
    <Color x:Key="Color.Secondary01.500">#559CE4</Color>

    <!--Brush-->
    <SolidColorBrush x:Key="Brush.White.alpha16" Color="{StaticResource Color.White.alpha16}"/>
    <SolidColorBrush x:Key="Brush.Secondary01.500" Color="{StaticResource Color.Secondary01.500}"/>

</ResourceDictionary>